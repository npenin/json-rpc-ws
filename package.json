{
  "name": "@akala/json-rpc-ws",
  "version": "7.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:npenin/json-rpc-ws.git"
  },
  "description": "json-rpc websocket transport",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "test": "lab -a code -v -e test",
    "validate": "npm ls",
    "jsdoc": "jsdoc",
    "docs": "jsdoc index.js lib -d docs",
    "preversion": "git checkout master && git pull origin master && npm ls",
    "publish-patch": "npm run preversion && npm version patch && git push origin master --tags && npm publish",
    "publish-minor": "npm run preversion && npm version minor && git push origin master --tags && npm publish",
    "publish-major": "npm run preversion && npm version major && git push origin master --tags && npm publish",
    "prepublishOnly": "tsc -p src"
  },
  "keywords": [
    "json-rpc",
    "websocket"
  ],
  "author": "Gar <gar+npm@danger.computer>",
  "license": "MIT",
  "dependencies": {
    "debug": "^3.1.0",
    "uuid": "^3.3.2",
    "ws": "^6.0.0"
  },
  "pre-commit": [
    "validate",
    "lint",
    "test"
  ],
  "files": [
    "browser.js",
    "lib/*",
    "src/*"
  ],
  "browser": "./browser.js",
  "devDependencies": {
    "@types/assert": "0.0.31",
    "@types/debug": "0.0.30",
    "@types/node": "^10.5.8",
    "@types/uuid": "^3.4.3",
    "@types/ws": "^5.1.2",
    "ajv": "^6.5.2",
    "browserify": "^16.2.2",
    "bufferutil": "^4.0.0",
    "code": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-andyet": "^2.0.0",
    "eslint-plugin-hapi": "^4.1.0",
    "git-validate": "^2.2.4",
    "jsdoc": "^3.3.2",
    "lab": "^17.0.0",
    "phantomjs-prebuilt": "^2.1.16",
    "selenium-webdriver": "^3.6.0",
    "utf-8-validate": "^5.0.1"
  }
}